{
  "name": "api-server-clou-digital",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "lint": "eslint . --ext=.ts"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp"
    ],
    "rules": {
      "@typescript-eslint/semi": [
        2,
        "always"
      ],
      "semi": [
        2,
        "always"
      ],
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "quotes": [
        2,
        "single",
        {
          "avoidEscape": true
        }
      ],
      "max-len": "off"
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.5",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "^2.2.2",
    "@types/bull": "^4.10.0",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-adonis": "^2.1.1",
    "pino-pretty": "^9.1.1",
    "typescript": "~4.6",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/drive-s3": "^1.3.2",
    "@adonisjs/lucid": "^18.3.0",
    "@adonisjs/lucid-slugify": "^2.2.1",
    "@adonisjs/mail": "^8.1.2",
    "@adonisjs/redis": "^7.3.2",
    "@adonisjs/repl": "^3.1.11",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/view": "^6.2.0",
    "@babel/runtime": "^7.21.5",
    "@google-analytics/data": "^3.2.2",
    "@googlemaps/google-maps-services-js": "^3.3.28",
    "@rocketseat/adonis-bull": "^1.0.4",
    "@sentry/node": "^7.57.0",
    "@sentry/profiling-node": "^1.0.7",
    "@types/node": "^20.5.3",
    "adonis-lucid-soft-deletes": "^1.4.3",
    "adonis5-scheduler": "^2.0.2",
    "adonis5-sentry": "^3.1.0",
    "axios": "^1.3.3",
    "bull": "^4.10.4",
    "csvtojson": "^2.0.10",
    "http": "^0.0.1-security",
    "jssha": "^3.3.0",
    "luxon": "^3.2.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mysql2": "^3.2.4",
    "node-libcurl": "^3.0.0",
    "openai": "^3.2.1",
    "pg": "^8.8.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-ip": "^3.3.0",
    "socket.io": "^4.6.0",
    "source-map-support": "^0.5.21",
    "stripe": "^11.14.0",
    "twilio": "^4.15.0",
    "typesense": "^1.5.3",
    "useragent": "^2.3.0"
  }
}
