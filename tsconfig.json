{
  "extends": "adonis-preset-ts/tsconfig.json",
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "dom",
      "ES2021.String"
    ],
    "outDir": "build",
    "rootDir": "./",
    "sourceMap": true,
    "paths": {
      "App/*": [
        "./app/*"
      ],
      "Config/*": [
        "./config/*"
      ],
      "Contracts/*": [
        "./contracts/*"
      ],
      "Database/*": [
        "./database/*"
      ]
    },
    "types": [
      "@adonisjs/core",
      "@adonisjs/repl",
      "@japa/preset-adonis/build/adonis-typings",
      "@adonisjs/lucid",
      "@adonisjs/auth",
      "@adonisjs/mail",
      "@adonisjs/view",
      "adonis-lucid-soft-deletes",
      "@adonisjs/drive-s3",
      "adonis5-sentry",
      "@adonisjs/lucid-slugify",
      "adonis5-scheduler",
      "@adonisjs/redis",
      "@rocketseat/adonis-bull",
      "@adonisjs/session"
    ]
  }
}